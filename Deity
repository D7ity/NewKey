-- Scripts:

local function FEHRVTD_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	--hotbar functions
	local hotbar = script.Parent.Hotbar
	--login functions
	local login = script.Parent.Login
	local Scriptlogin = script.Parent.Hotbar.IconBACKGROUND.Login
	local exitbutton = login.Exitbutton
	local username = script.Parent.Login.Username
	local password = script.Parent.Login.Password
	local confirm = script.Parent.Login.Confirm
	--welcome gui functions
	local welcome = script.Parent.Welcome
	local homeinfo = script.Parent.Hotbar.IconBACKGROUND.HomeInfo
	local exitbutton1 = welcome.Exitbutton1 
	--credit functions
	local Credits = script.Parent.Hotbar.IconBACKGROUND.Credits
	local creditgui =script.Parent.Credits
	local exitbutton4 = creditgui.Exitbutton4
	--player functions
	local player = script.Parent.Hotbar.IconBACKGROUND.Player
	local playergui = script.Parent.Player
	local exitbutton3 = playergui.Exitbutton3
	
	--skidded script functions
	local skid = script.Parent.Hotbar.IconBACKGROUND.SkiddedScript
	--Hotbar Minimise and open
	local minimise = script.Parent.Hotbar.HotbarMinimise
	local openhotbar = script.Parent.OpenHotbar
	-- login script
	Scriptlogin.MouseButton1Click:Connect(function()
		login.Visible = true	
	end)
	
	exitbutton.MouseButton1Click:Connect(function()
		login.Visible = false
	end)
	
	login.Active = true
	login.Draggable = true
	
	--welcome gui
	
	homeinfo.MouseButton1Click:Connect(function()
		welcome.Visible = true
	end)
	
	exitbutton1.MouseButton1Click:Connect(function()
		welcome.Visible = false
	end)
	
	welcome.Active = true
	welcome.Draggable = true
	
	--script
	confirm.MouseButton1Down:Connect(function()
		if username.Text == "2" then
			if password.Text == "1" then
				Credits.Visible = true
				player.Visible = true
				skid.Visible = true
				login.Visible = false
				Scriptlogin.Visible = false
				homeinfo.Position = UDim2.new(0.264, 0,0.274, 0)
		   end	
		end
	end)
	
	skid.MouseButton1Click:Connect(function()
		local window = loadstring(game:HttpGet('https://raw.githubusercontent.com/D7ity/Keylesss/main/Hub'))()
	end)
	--player
	
	player.MouseButton1Click:Connect(function()
		playergui.Visible = true
		playergui.Active = true
		playergui.Draggable = true
	end)
	
	exitbutton3.MouseButton1Click:Connect(function()
		playergui.Visible = false
	end)
	
	--credits
	Credits.MouseButton1Click:Connect(function()
		creditgui.Visible = true
		creditgui.Active = true
		creditgui.Draggable = true
	end)
	
	
	exitbutton4.MouseButton1Click:Connect(function()
		creditgui.Visible = false
	end)
	
	--minimise script
	minimise.MouseButton1Click:Connect(function()
		hotbar.Visible = false
		openhotbar.Visible = true
		openhotbar.Active = true
		openhotbar.Draggable = true
		
	end)
	
	openhotbar.MouseButton1Click:Connect(function()
		hotbar.Visible = true
		openhotbar.Visible = false
		minimise.Active = true
		minimise.Draggable = true	
		hotbar.Active = true
		hotbar.Draggable = true
	end)
end
coroutine.wrap(FEHRVTD_fake_script)()
local function LJTKEKI_fake_script() -- Welcome.LocalScript 
	local script = Instance.new('LocalScript', Welcome)

	local welcome = script.Parent
	
	
	
	local player = game.Players.LocalPlayer
	
	
	
	local userId = player.UserId
	
	local thumbType = Enum.ThumbnailType.AvatarBust
	
	local thumbSize = Enum.ThumbnailSize.Size420x420
	
	local content, isReady = game.Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	
	
	
	
	welcome.PlayerImage.Image = content
	
	welcome.PlayerName.Text = player.DisplayName .. ", Welcome To Divinity"
	
end
coroutine.wrap(LJTKEKI_fake_script)()
local function BOXQVMW_fake_script() -- time.LocalScript 
	local script = Instance.new('LocalScript', time)

	while true do
		local TimeInUnix = os.time()
		local Date = script.Parent
		
		local StringToFormat = "%I:%M %p"
		
		local result = os.date(StringToFormat, TimeInUnix)
		
		Date.Text = result
		wait(1)
	end
end
coroutine.wrap(BOXQVMW_fake_script)()
